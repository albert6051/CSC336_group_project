<head>
        <% include ../partials/style%>
    </head>
    
    <body id="page-top">
    
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top" id="">
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav">
              <li class="nav-item">
                  <a class="nav-link js-scroll-trigger" href="http://localhost:3003">Main Page</a>
              </li>
              <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#contact">Contact Info</a>
              </li>
              <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#business">Business</a>
              </li>
              <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#jobs">Job Posted</a>
              </li>
            </ul>
          </div>
        </nav>
      
        <div class="container">
          <section class="resume-section p-lg-5" id="contact">
            <div class="w-100">
              <h2 class="mb-3"><%= business.username %></h2>
              <div class="lead mb-5">
                <h3>Contact:</h3>
                  <label>Email: </label>
                  <a href="#"><%= business.email %></a><br>
                  <label>Phone: </label>
                  <text><%= business.phone_number %></text><br>
                  <label>Address: </label>
                  <text><%= business.address %></text>
              </div>
            </div>
          </section>
      
          <hr class="m-0">
      
          <section class="resume-section p-3 p-lg-5 d-flex justify-content-center" id="business">
            <div class="w-100">
                <div>
                    <h2>Business</h2>
                    <div class="resume-item d-flex flex-column flex-md-row justify-content-between mb-5">
                        <div class="resume-content">
                            <p class="lead"><%= business.business %></p>
                        </div>
                    </div>
              </div>
            </div>
          </section>
      
          <hr class="m-0">
      
          <section class="resume-section p-3 p-lg-5 d-flex align-items-center" id="jobs">
            <div class="w-100">
              <h2 class="mb-5">Job Posted</h2>
              <% results.forEach(function(job) { %>
              <div class="">
                  <a href='/job_des/<%= job.job_ID%>' class="lead"><%= job.job_title %></a><br>
              </div>
              <% }); %>
      
            </div>
          </section>
      
          <hr class="m-0">
    
        </div>
      
      </body>